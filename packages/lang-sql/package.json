{
  "name": "@dortdb/lang-sql",
  "type": "module",
  "version": "1.1.1",
  "description": "SQL parser and executor for DortDB",
  "exports": {
    ".": "./dist/index.js",
    "./ast": "./dist/ast/index.js",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filipjezek/dortdb.git"
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "peggy": "peggy -c src/parser/peggy-config.mjs"
  },
  "author": "dortdb",
  "license": "ISC",
  "nx": {
    "sourceRoot": "packages/lang-sql/src",
    "targets": {
      "build": {
        "options": {
          "assets": [
            {
              "glob": "parser/sql.peggy.{mjs,d.ts,mjs.map}",
              "input": "{projectRoot}/src",
              "output": "."
            }
          ]
        }
      }
    }
  },
  "peerDependencies": {
    "@dortdb/core": "^1.0.0"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.15",
    "es-toolkit": "^1.43.0",
    "tslib": "^2.6.2"
  }
}
