{
  "name": "@dortdb/lang-cypher",
  "type": "module",
  "version": "1.0.0",
  "description": "Cypher parser and executor for DortDB",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run jison && node --import tsx --test \"tests/**/*.spec.ts\"",
    "antlr": "antlr4ng -Dlanguage=TypeScript -o src/parser/generated/ -visitor -no-listener -long-messages -Xexact-output-dir src/parser/Cypher.g4",
    "rollup": "rollup -c rollup.config.js",
    "rollup:watch": "rollup -c rollup.config.js --watch",
    "build": "npm run jison && npm run rollup",
    "start": "node dist/index.js"
  },
  "author": "dortdb",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/emscripten": "^1.39.10",
    "@types/node": "^22.2.0",
    "antlr4ng-cli": "^2.0.0",
    "esbuild": "^0.20.2",
    "rollup": "^4.14.3",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "tsx": "^4.17.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@dortdb/core": "^1.0.0"
  },
  "dependencies": {
    "antlr4ng": "^3.0.7",
    "tslib": "^2.6.2"
  }
}
